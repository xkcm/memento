{
  "name": "@xkcm/memento",
  "version": "1.0.0-beta",
  "license": "MIT",
  "author": {
    "name": "xkcm",
    "email": "xkcm16+npm@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xkcm/memento.git"
  },
  "description": "TypeScript library for memoization",
  "files": [
    "./lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./lib/es6/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/es6/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "./": [
        "./lib/es6/index.d.ts"
      ]
    }
  },
  "keywords": [
    "typescript",
    "ts",
    "cache",
    "javascript",
    "js",
    "caching",
    "utils"
  ],
  "scripts": {
    "test": "vitest",
    "build": "rm -rf ./lib && tsc -p tsconfig.build.es6.json && tsc -p tsconfig.build.cjs.json",
    "lint": "pnpm exec eslint ./src ./tests --ext .ts"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/sha256": "^0.2.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@node-redis/client": "^1.0.6",
    "@xkcm/better-errors": "^2.2.0",
    "sha256": "^0.2.0",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
